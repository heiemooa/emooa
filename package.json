{
  "name": "emooa",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/heiemooa/emooa",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "test": "lerna run test --stream",
    "build": "lerna run build",
    "exec": "lerna exec -- yarn",
    "publish": "lerna run build && lerna publish",
    "local:build": "yarn run build && rm -rf node_modules && yarn run exec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heiemooa/emooa.git"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.44",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "dumi": "^2.2.16",
    "lerna": "^8.0.0",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}
